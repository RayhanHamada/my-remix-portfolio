{
  "private": true,
  "name": "web-client",
  "description": "My Remix Portfolio",
  "license": "MIT",
  "author": "Muhammad Rayhan Hamada Budiman <rayhanhamada1999@gmail.com>",
  "repository": "https://github.com/RayhanHamada/my-remix-portfolio",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "windicss app/{**/*.tsx,root.tsx} -m -b -o ./app/windi.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "windicss app/{**/*.tsx,root.tsx} -m -d -b -o ./app/windi.css",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "rmrf-build": "rimraf build/ public/build/",
    "rmrf-clean": "rimraf build/ public/build/ app/windi.css"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.3.1",
    "octokit": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/serve": "^1.3.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "concurrently": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "windicss": "^3.5.1"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
